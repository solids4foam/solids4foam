FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-8;
        relTol          0.001;
        minIter         1;
    }

    pFinal
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-8;
        relTol          0.001;
        minIter         1;
    }

    U
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-5;
        relTol          0;
        minIter         1;
    }

    UFinal
    {
        solver          BiCGStab;
        preconditioner  DILU;
        tolerance       1e-05;
        relTol          0;
        minIter         1;
    }
}

PISO
{
    nOuterCorrectors 5;
    nCorrectors 3;
    nNonOrthogonalCorrectors 1;
}

PIMPLE
{
    nOuterCorrectors 5;
    nCorrectors 3;
    nNonOrthogonalCorrectors 1;
}

relaxationFactors
{
    U 0.7;
    UFinal 0.7;
    p 0.3;
}

