FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    object      pointMotionU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    symmetry-x
    {
        type         symmetryPlane;
    }
    symmetry-y
    {
        type         symmetryPlane;
    }
    outlet
    {
        type         fixedValue;
        value        uniform (0 0 0);
    }
    inlet
    {
        type         fixedValue;
        value        uniform (0 0 0);
    }
    wall
    {
        type         fixedValue;
        value        uniform (0 0 0);
    }
}

