FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    symmetry-x
    {
        type            symmetryPlane;
    }
    symmetry-y
    {
        type            symmetryPlane;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    inlet
    {
        // openfoam
        //type          uniformFixedValue;
        uniformValue     tableFile;

        // foam extend
        type        timeVaryingUniformFixedValue;
        file   	        "$FOAM_CASE/0/fluid/time-series";
        outOfBounds	clamp;
    }
    wall
    {
        type            zeroGradient;
        value      	uniform 0;
    }

}

