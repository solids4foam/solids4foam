FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      mechanicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

planeStress no;

mechanical
(
    pipe
    {
        type            linearElastic;
        //type            StVenantKirchhoffElastic;
        rho             rho [1 -3 0 0 0 0 0]  1200;
        E               E   [1 -1 -2 0 0 0 0] 3e5;
        nu              nu  [0 0 0 0 0 0 0]   0.3;
    }
);

