FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.005;

boundaryField
{
    plate
    {
        type            kqRWallFunction;
        value           uniform 0.005;
    }
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 0.005;
    }
    cylinder
    {
        type            kqRWallFunction;
        value           uniform 0.005;
    }
    bottom
    {
        type            kqRWallFunction;
        value           uniform 0.005;
    }
    top
    {
        type            kqRWallFunction;
        value           uniform 0.005;
    }
    frontAndBackPlanes
    {
        type            empty;
    }
}


