FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      mechanicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

planeStress no;

mechanical
(
    fluidMesh
    {
        type        diffusionElastic;
        rho         rho [1 -3 0 0 0 0 0] 1;
        mu          mu [1 -1 -2 0 0 0 0] 1;
        kappa       kappa [1 -1 -2 0 0 0 0] 3;
        regionName  meshMotionFluid;
        diffusivity quadratic inverseDistance
        (
            interface top bottom left inlet outlet
        );
        //diffusivity inverseDistance (interface);
        //diffusivity uniform;

        // type        linearElastic;
        // rho         rho [1 -3 0 0 0 0 0] 1;
        // E           E [1 -1 -2 0 0 0 0] 1;
        // nu          nu [0 0 0 0 0 0 0] 0.3;
        // regionName  meshMotionFluid;
    }
);

