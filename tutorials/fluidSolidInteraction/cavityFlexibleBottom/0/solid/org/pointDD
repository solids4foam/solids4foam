FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    object      pointDD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    interface
    {
        type           calculated;
	value          uniform (0 0 0);
    }
    bottom
    {
        type           calculated;
        value          uniform (0 0 0);
    }
    right
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }
    left
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }/*
    symmetry
    {
       type            symmetry;
       value           uniform (0 0 0);
       }*/
}

