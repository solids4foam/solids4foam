FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       pimpleFluid;
    location    "constant/fluid";
    object      fluidProperties.withDefaultValues;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

fluidModel      pimpleFluid;

pimpleFluidCoeffs
{
    solveEnergyEq   false;
}


