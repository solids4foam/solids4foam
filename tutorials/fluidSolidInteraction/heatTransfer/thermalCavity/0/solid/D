FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      D;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    top
    {
        type            fixedDisplacement;
        value           uniform (0 0 0);
        // type            tractionExtrapolatedDisplacement;
        // traction        uniform ( 0 0 0 );
        // pressure        uniform 0.001;
        // value           uniform (0 0 0);
    }

    left
    {
        type            fixedDisplacement;
        value           uniform (0 0 0);
    }

    right
    {
        type            fixedDisplacement;
        value           uniform (0 0 0);
    }

    bottom
    {
        type            fixedDisplacement;
        value           uniform (0 0 0);
        // type            tractionExtrapolatedDisplacement;
        // traction        uniform ( 0 0 0 );
        // pressure        uniform 0;
        // value           uniform (0 0 0);
    }

    frontAndBack
    {
        type            empty;
    }
}


