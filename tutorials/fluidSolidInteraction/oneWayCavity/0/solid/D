FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      D;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    interface
    {
        type            solidTraction;
        traction        uniform ( 0 0 0 );
        pressure        uniform 0;
        value           uniform (0 0 0);
    }
    top
    {
       type            fixedDisplacement;
       value           uniform (0 0 0);
    }
    outside
    {
       type            solidTraction;
       traction        uniform (0 0 0);
       pressure        uniform 0;
       value           uniform (0 0 0);
    }
    frontAndBack
    {
        type           empty;
    }
}

// ************************************************************************* //
