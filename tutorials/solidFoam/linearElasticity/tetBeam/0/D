FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      D;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    top
    {
        type            blockSolidTraction;
        traction        uniform ( 0 1e7 0 );
        pressure        uniform 0;
        value           uniform (0 0 0);
    }

    bottom
    {
        type            blockFixedDisplacement;
        value           uniform ( 0 0 0 );
    }

    defaultFaces
    {
        type            blockSolidTraction;
        traction        uniform ( 0 0 0 );
        pressure        uniform 0;
        value           uniform (0 0 0);
    }
}
// ************************************************************************* //
