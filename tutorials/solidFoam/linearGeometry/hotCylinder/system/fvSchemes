FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

d2dt2Schemes
{
    default            steadyState;
}

ddtSchemes
{
    default            steadyState;
}

gradSchemes
{
    default            extendedLeastSquares 0;
}

divSchemes
{
    default            Gauss linear;
}

laplacianSchemes
{
    default            none;
    laplacian(DD,D)    Gauss linear newSkewCorrected 1;
    laplacian(DDD,DD)  Gauss linear newSkewCorrected 1;
    laplacian(k,T)     Gauss linear newSkewCorrected 1;
}

snGradSchemes
{
    default            none;
    snGrad(D)          newSkewCorrected 1;
    snGrad(DD)         newSkewCorrected 1;
}

interpolationSchemes
{
    default            none;
    interpolate(impK)  linear;
}

// ************************************************************************* //
