(
    arc 0 1 (23.33452378 -23.33452378 -0.5)
    arc 4 5 (23.33452378 -23.33452378 0.5)

    arc 2 3 (21.92031022 -21.92031022 -0.5)
    arc 6 7 (21.92031022 -21.92031022 0.5)
);

boundary
(
    topAndBottom
    {
        type patch;
        faces
        (
            (7 6 2 3)
            (4 0 1 5)
        );
    }

