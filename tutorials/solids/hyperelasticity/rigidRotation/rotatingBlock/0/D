//         pressure        uniform 0;
//         value           uniform (0 0 0);
    }

    top
    {
        type           fixedRotation;
        rotationAngleSeries
        {
            fileName       "$FOAM_CASE/constant/timeVsAngle";
            outOfBounds    clamp;
        }
        rotationAxis   ( 0 0 1 );
        rotationOrigin ( 0 0 0 );
        value          uniform ( 0 0 0 );
//         type            solidTraction;
//         traction        uniform ( 0 0 0 );
//         pressure        uniform 0;
//         value           uniform (0 0 0);
    }

    bottom
    {
        type           fixedRotation;
        rotationAngleSeries
        {
            fileName       "$FOAM_CASE/constant/timeVsAngle";
            outOfBounds    clamp;
