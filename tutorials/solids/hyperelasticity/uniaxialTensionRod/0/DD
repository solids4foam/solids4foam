FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      DD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    loading
    {
        type            fixedDisplacement;
        //type            fixedDisplacementZeroShear;
        displacementSeries
        {
            "fileName|file"        "$FOAM_CASE/constant/timeVsDisp";
            outOfBounds     clamp;
        }
        value           uniform (0 0 0);
    }

    tracFree
    {
        type            solidTraction;
        traction        uniform (0 0 0);
        pressure        uniform 0;
        value           uniform (0 0 0);
    }

    symmPlane
    {
        type            solidSymmetry;
        patchType       symmetryPlane;
        value           uniform (0 0 0);
    }

    back
    {
        type            solidSymmetry;
        patchType       symmetryPlane;
        // type            solidWedge;
        // patchType       wedge;
        value           uniform (0 0 0);
    }

    front
    {
        type            solidSymmetry;
        patchType       symmetryPlane;
        // type            solidWedge;
        // patchType       wedge;
        value           uniform (0 0 0);
    }

    axis
    {
        type            empty;
    }
}

