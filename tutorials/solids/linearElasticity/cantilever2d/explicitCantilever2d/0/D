FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      D;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    right
    {
        type            solidTraction;
        traction        uniform ( 1e6 -1e6 0 );
        pressure        uniform 0;
        value           uniform (0 0 0);
    }

    top
    {
        type            solidTraction;
        traction        uniform ( 0 0 0 );
        pressure        uniform 0;
        value           uniform (0 0 0);
    }

    bottom
    {
        type            solidTraction;
        traction        uniform ( 0 0 0 );
        pressure        uniform 0;
        value           uniform (0 0 0);
    }

    left
    {
        type            fixedDisplacement;
        value           uniform ( 0 0 0 );
    }

    back
    {
         type            empty;
    }

    front
    {
         type            empty;
    }
}

// ************************************************************************* //
