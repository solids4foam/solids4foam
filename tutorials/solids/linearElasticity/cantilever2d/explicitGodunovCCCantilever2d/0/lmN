FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    location    "0";
    object      lmN;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    top
    {
        type    zeroGradient;
    }
    
    bottom
    {
        type    zeroGradient;
    }

    
    right
    {
        type    zeroGradient;
    }

   left
   {
       type    fixedValue;
       value   uniform (0 0 0);
   }

   back
   {
        type            empty;
   }

   front
   {
        type            empty;
   }
}

