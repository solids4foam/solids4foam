FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      lm_b;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    top
    {
        type    tractionLinearMomentum;
        loadingType		traction;
        traction		(0 0 0);
        value 		uniform (0 0 0);
    }
    
    bottom
    {
        type    tractionLinearMomentum;
        loadingType		traction;
        traction		(0 0 0);
        value 		uniform (0 0 0);
    }

    right
     {
        type    tractionLinearMomentum;
        loadingType	traction;
        traction     ( 0 -1e6 0 );
        value 		uniform (0 0 0);
    }

    left
    {
        type    fixedValue;
        value   uniform (0 0 0);
    }

    back
    {
         type            empty;
    }

    front
    {
         type            empty;
    }
    
}

