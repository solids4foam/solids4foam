FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      t_b;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
  
    top
    {
        type    tractionTraction;
        loadingType		traction;
        traction		(0 0 0);
        value 		uniform (0 0 0);
    }
    
    bottom
    {
        type    tractionTraction;
        loadingType		traction;
        traction		(0 0 0);
        value 		uniform (0 0 0);
    }

    
    right
     {
        type    		tractionTraction;
        loadingType		traction;
        traction     ( 0 -1e6 0 );
        value 			uniform (0 0 0);
    }

    left
    {
        type    movingTraction;
        value   uniform (0 0 0);
    }

    back
    {
         type            empty;
    }

    front
    {
         type            empty;
    }
    
}

