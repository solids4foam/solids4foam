FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs
(
    "libcantileverDisplacementPointPatch.so"
    "libcantileverTractionPointPatch.so"
);

application     solids4Foam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1;

deltaT          1;

writeControl    timeStep;

writeInterval   1;

cycleWrite      0;

writeFormat     binary;

writePrecision  12;

writeCompression uncompressed;

timeFormat      general;

timePrecision   12;

runTimeModifiable no;

functions
{
    cantileverAnalytical
    {
        type cantileverAnalyticalSolution;
        P 0.1e6;
        E 200e9;
        nu 0.3;
        L 2;
        D 0.1;
        cellDisplacement no;
        pointDisplacement yes;
        cellStress yes;
        pointStress no;
    }
}
// ************************************************************************* //
