FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

d2dt2Schemes
{
    default                 Euler;
    //default                 backward;
}

ddtSchemes
{
    default                 Euler;
    //default                 backward;
}

gradSchemes
{
     default                none;
     grad(D)                extendedLeastSquares 0;
}

divSchemes
{
    default                 none;
    div(sigma)              Gauss linear;
}

laplacianSchemes
{
    default                 none;
    laplacian(DD,D)         Gauss linear corrected;
}

snGradSchemes
{
    default                 none;
}

interpolationSchemes
{
    default                 none;
    interpolate(impK)       linear;
    interpolate(grad(D))    linear;
}

