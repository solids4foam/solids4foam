FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs            ("libsampling.so");

application     solids4Foam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1.6e-03;

deltaT          1e-07;

maxCo           0.1;

writeControl    runTime;

writeInterval   1.6e-04;

cycleWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable no;

functions
{
    probe
    {
        type           probes;
        outputControl  timeStep;
        outputInterval 1;
        fields         (D sigma);
        probeLocations ((0.1 0 0));
    }
}

// ************************************************************************* //
