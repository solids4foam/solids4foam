FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     solids4Foam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.36;

deltaT          0.01;

writeControl    runTime;

writeInterval   0.01;

// maxCo is only used by the explicit solid models
maxCo           0.05;

cycleWrite      0;

writeFormat     binary;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable no;

functions
{
    pointDisp
    {
        type    solidPointDisplacement;
        point   (0.0408 0.0048 0.0929);
    }
}

