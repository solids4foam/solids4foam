FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      D;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inner
    {
        type            solidTraction;
        traction        uniform (0 0 0);
        pressure        uniform 1e5;
        value           uniform (0 0 0);
    }
    outer
    {
         type            solidTraction;
         traction        uniform (0 0 0);
         pressure        uniform 0;
         value           uniform (0 0 0);
    }
    symmX
    {
        type            symmetry;
    }
    symmY
    {
        type            symmetry;
    }
    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
