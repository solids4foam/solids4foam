FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    system;
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

setFormat raw;

surfaceFormat vtk;

interpolationScheme cellPointFace;

fields
(
    sigma
);

sets
(
    lineXX
    {
        type uniform;
        axis            x;
        nPoints         50;
        start (0.05 1e-6 0.0005);
        end   (0.1 1e-6 0.0005);
    }
    lineYY
    {
        type uniform;
        axis            y;
        nPoints         50;
        start (1e-6 0.05 0.0005);
        end   (1e-6 0.1 0.0005);
    }
);

surfaces
();


