FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      D;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    bottom
    {
        type            slip;
    }

    sides
    {
        type            slip;
    }

    seabed
    {
        type            solidTraction;
        traction        uniform (0 0 0);
        pressure        uniform  0;
        value           uniform (0 0 0);
    }
}

