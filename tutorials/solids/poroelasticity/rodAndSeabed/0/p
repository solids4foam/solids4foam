FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 79290;

boundaryField
{
    bottom
    {
        type            zeroGradient;
    }

    sides
    {
        type            zeroGradient;
    }

    seabed
    {
        type            timeVaryingMappedFixedValue;
        setAverage      off;
    }
}

